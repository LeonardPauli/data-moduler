{
  "name": "data-moduler",
  "version": "1.0.0",
  "description": "Automatic generation of database models, rest endpoints and graphql api (all with validations, ease of authentisation and fully customizable) from simple specification \"module\" files",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon -q examples/index.js --exec babel-node",
    "debug": "nodemon examples/index.js --exec babel-node --inspect",
    "start": "babel-node examples/index.js",
    "build": "babel src -d dist/src & babel examples -d dist/examples",
    "serve": "forever dist/index.js",
    "lint": "eslint --ext .js src,examples"
  },
  "author": "Leonard Pauli",
  "private": true,
  "engines": {
    "node": ">=6.11.0",
    "npm": ">=3.10.10",
    "yarn": ">=0.27.5"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.7.0"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "faker": "^4.1.0",
    "graphiql": "^0.11.2",
    "graphql": "^0.10.3",
    "jsonwebtoken": "^7.4.1",
    "lodash": "^4.17.4",
    "passport": "^0.3.2",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.2.1"
  }
}
